}
@-webkit-keyframes showPopup {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
@keyframes showPopup {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
@-webkit-keyframes hidePopup {
0% {
    opacity: 1;
}
100% {
    opacity: 0;
}
}
@keyframes hidePopup {
0% {
    opacity: 1;
}
100% {
    opacity: 0;
}
}
@-webkit-keyframes showLayer {
0% {
    -webkit-transform: translateY(120%);
            transform: translateY(120%);
}
100% {
